<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>	
	<!--SOP模板管理-->
	<select id="pages.sysmgr.sop.busstatusBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT * 
		FROM sop_bustate 
		WHERE isdelete = FALSE order BY id;	
	</select>
	<select id="pages.sysmgr.sop.busstatusBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		select 
		count(*) AS counts 
		FROM sop_bustate s 
		WHERE s.isdelete = FALSE; 	
	</select>
</sqlMap>
