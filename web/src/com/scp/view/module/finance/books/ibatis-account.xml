<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<select id="pages.module.finance.books.ledgerBean.grid.page"
		parameterClass="java.util.Map" resultClass="java.util.HashMap">
		select
			*
		FROM sys_report t
		WHERE $qry$
			AND modcode = 'arap'
		ORDER BY namec DESC
	</select>
	<select id="pages.module.finance.books.ledgerBean.grid.count"
		parameterClass="java.util.Map" resultClass="java.util.HashMap">
		select
		count(*) AS counts
		FROM sys_report t
		WHERE $qry$
		AND modcode = 'arap' 
	</select>
	
	<select id="pages.module.finance.books.astdtlBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			* 
		FROM _fs_ast t 
		WHERE $qry$ 
		ORDER BY code
		LIMIT $limit$ OFFSET $start$
	</select>
	<select id="pages.module.finance.books.astdtlBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			COUNT(*) AS counts
		FROM _fs_ast t 
		WHERE $qry$ 
	</select>
	
	
	
	<select id="pages.module.finance.books.daydetailBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
		* 
		FROM f_rpt_fs_getbooksday($filter$) a;
	</select>
	<select id="pages.module.finance.books.daydetailBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			100000 AS counts
		
	</select>
	
	<select id="pages.module.finance.books.detailgridBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
		* 
		FROM f_rpt_fs_getbooksdetail($filter$) a;
	</select>
	<select id="pages.module.finance.books.detailgridBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			100000 AS counts
		
	</select>
	

</sqlMap>
