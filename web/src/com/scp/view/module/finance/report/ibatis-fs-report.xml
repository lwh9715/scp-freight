<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<select id="pages.module.finance.report.fs_feeBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			* 
		FROM _fs_ast t 
		WHERE $qry$ 
			$filter$
		ORDER BY code
	</select>
	<select id="pages.module.finance.report.fs_feeBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			COUNT(*) AS counts
		FROM _fs_ast t 
		WHERE $qry$ 
			$filter$
	</select>
	
	<select id="pages.module.finance.report.fs_fee_profitBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			id,code,namec,namee,abbr 
		FROM sys_corporation t 
		WHERE
			$qry$
			AND t.isdelete = false 
		ORDER BY code
		LIMIT $limit$ OFFSET $start$
	</select>
	<select id="pages.module.finance.report.fs_fee_profitBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT 
			COUNT(*) AS counts
		FROM sys_corporation t 
		WHERE
			$qry$ 
			AND t.isdelete = false 
	</select>

	<select id="pages.module.finance.report.fs_profit_lossBean.grid.page" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT
			*
		FROM _fs_ast t
		WHERE $qry$
			$filter$
		ORDER BY code
	</select>
	<select id="pages.module.finance.report.fs_profit_lossBean.grid.count" parameterClass="java.util.Map" resultClass="java.util.HashMap">
		SELECT
			COUNT(*) AS counts
		FROM _fs_ast t
		WHERE $qry$
			$filter$
	</select>
</sqlMap>
